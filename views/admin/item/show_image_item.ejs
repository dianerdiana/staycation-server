<div
  class="tab-pane fade show active"
  id="nav-image-item"
  role="tabpanel"
  aria-labelledby="nav-image-item-tab"
>
  <div class="row mt-2">
    <% for (let x = 0; x < item.imageId.length; x++) { %>
    <div class="col md-4">
      <!-- DataTables Example -->
      <div class="card shadow mb-4">
        <div class="card-body">
          <img
            src="<% process.env.APP_URL %>/<%= item.imageId[x].imageUrl %>"
            alt="image"
            width="100%"
            style="display: block"
          />
        </div>
      </div>
    </div>
    <% } %>
  </div>
  <a href="/admin/item" class="btn btn-warning"> Back </a>
</div>
